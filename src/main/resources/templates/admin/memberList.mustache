{{>layout/header}} <!-- >는 현재 머스테치 파일을 기준으로 다른 파일을 가져옴. -->
<!-- bradcam_area  -->
<div class="bradcam_area bradcam_bg_3">
    <div class="container">
        <div class="row">
            <div class="col-xl-12">
                <div class="bradcam_text">
                    <h3>유저 목록</h3>
                </div>
            </div>
        </div>
    </div>
</div>
<!--/ bradcam_area  -->

<!-- ================ contact section start ================= -->
<section class="contact-section section_padding">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-12">
                <div class="def_mar">
                    <h1>유저 목록</h1>
                </div>
                <table class="table table-horizontal" table-border>
                    <thead class="thead-strong">
                    <tr>
                        <th>아이디</th>
                        <th>비밀번호</th>
                        <th>이메일</th>
                        <th>이름</th>
                        <th>폰번호</th>
                        <th>등록날짜</th>
                    </tr>
                    </thead>
                    <tbody id="tbody">
                    {{#requestList}}
                        <tr>
                            <td>{{id}}</td>
                            <td><a href="/happyParking/requestHappyParking/{{parkingName}}">{{parkingName}}</a></td>
                            <td>{{parkingType}}</td>
                            <td>{{place}}</td>
                            <td>{{SAMPLE4_ROADADDRESS}}</td>
                            <td>{{SAMPLE4_DETAILADDRESS}}</td>
                            <td>{{SAMPLE4_EXTRAADDRESS}}</td>
                            <td>{{PARKINGDATE}}</td>
                        </tr>
                    {{/requestList}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>
{{>layout/footer}} <!-- >는 현재 머스테치 파일을 기준으로 다른 파일을 가져옴. -->