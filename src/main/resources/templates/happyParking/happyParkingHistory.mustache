{{>layout/header}}  <!-- >는 현재 머스테치 파일을 기준으로 다른 파일을 가져옴. -->
<div class="bradcam_area bradcam_bg_3">
    <div class="container">
        <div class="row">
            <div class="col-xl-12">
                <div class="bradcam_text">
                    <h3>주차장 이용내역</h3>
                </div>
            </div>
        </div>
    </div>
</div>

<section class="contact-section section_padding">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-12">
                <div class="def_mar">
                    <h1>주차장 이용내역</h1>
                    <a href="/chargingStation/chargingHistory" role="button" class="genric-btn primary circle">주차장 이용내역 보기</a>
                </div>
                <table class="table table-horizontal" table-border>
                    <thead class="thead-strong">
                    <tr>
                        <th>아이디</th>
                        <th>주차장명</th>
                        <th>이용날짜</th>
                        <th>주차위치</th>
                        <th>도로명주소</th>
                        <th>상세주소</th>
                        <th>여분주소</th>
                    </tr>
                    </thead>
                    <tbody id="tbody">
                    {{#requestList}}
                        <tr>
                            <td>{{id}}</td>
                            <td><a href="/happyParking/requestHappyParking/{{parkingName}}">{{parkingName}}</a></td>
                            <td>{{parkingType}}</td>
                            <td>{{place}}</td>
                            <td>{{SAMPLE4_ROADADDRESS}}</td>
                            <td>{{SAMPLE4_DETAILADDRESS}}</td>
                            <td>{{SAMPLE4_EXTRAADDRESS}}</td>
                            <td>{{PARKINGDATE}}</td>
                        </tr>
                    {{/requestList}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>

{{>layout/footer}}